{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "University Ranking vs Population",
    "data": {
    "url": "timesData.csv"
    },
    "mark": "circle",
    "encoding": {
    "x": {
    "field": "num_students",
